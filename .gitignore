# Laravel basic ignores

/vendor
/node_modules

# Environment files
.env
.env.backup
.env.production

# Laravel storage & cache

# Ignore all .key files (like encryption keys)
storage/*.key

# But include everything else in storage (logs, uploads, etc.)
!storage/**

# Public storage symlink folder (ignore if you use this)
public/storage

# Laravel cache & compiled files
/public/build
/public/hot

# PHPUnit
.phpunit.cache
.phpunit.result.cache

# IDE and editor folders
/.idea
/.vscode
/.fleet

# Composer & npm logs and metadata
auth.json
npm-debug.log
yarn-error.log

# Homestead files (Laravel VM)
Homestead.json
Homestead.yaml
